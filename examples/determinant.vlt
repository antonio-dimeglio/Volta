fn determinant_2x2(matrix: [[i32; 2]; 2]) -> i32 {
    return matrix[0][0] * matrix[1][1] - matrix[0][1] * matrix[1][0];
}

fn main() -> i32 {
    let matrix: [[i32; 2]; 2] = [[3, 8], [4, 6]];

    let det := determinant_2x2(matrix);

    // Return absolute value for exit code (since exit codes are 0-255)
    if det < 0 {
        return 0 - det;
    }
    return det;
}
