fn is_prime(n: i32) -> bool {
    if n < 2 { return false; }
    
    let mut i := 2;
    while i * i <= n {
        if n % i == 0 {
            return false;
        }
        i++;
    }
    return true;
}

fn main() -> i32 {
    let mut count := 0;
    let mut n := 2;
    while n < 100 {
        if is_prime(n) {
            count++;
        }
        n++;
    }
    return count;
}